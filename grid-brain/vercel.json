{
    "routes": [
        {
            "src": "/api",
            "dest": "api/index.py"
        },
        {
            "src": "/api/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/(.*\\.(exe|sh|dmg|AppImage))",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.html"
        }
    ]
}